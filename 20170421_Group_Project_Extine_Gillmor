        <script>
            $(document).ready(function(){
                $("#navItem1").mouseenter(function(){
                   $("#item1Table").css("display", "visible");
                   $("#navBarInfo").css("display", "visible");
                   $("#navItem1").mouseleave(function(){
                       $("#item1Table").css("display", "hidden");
                   });
                }); 
                $("#navItem2").mouseenter(function(){
                   $("#item2Table").css("display", "visible");
                   $("#navBarInfo").css("display", "visible");
                   $("#navItem2").mouseleave(function(){
                       $("#item2Table").css("display", "hidden");
                   });
                });
                $("#navItem3").mouseenter(function(){
                   $("#item3Table").css("display", "visible");
                   $("#navBarInfo").css("display", "visible");
                   $("#navItem3").mouseleave(function(){
                       $("#item3Table").css("display", "hidden");
                   });
                });
                $("#navItem4").mouseenter(function(){
                   $("#item4Table").css("display", "visible");
                   $("#navBarInfo").css("display", "visible");
                   $("#navItem4").mouseleave(function(){
                       $("#item4Table").css("display", "hidden");
                   });
                });
                $("#link1").mouseover(function(){
                    ("#link1").css("background-color", "yellow");
                });
            });
        </script>
